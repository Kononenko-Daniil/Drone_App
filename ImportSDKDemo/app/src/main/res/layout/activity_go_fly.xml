<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GoFlyActivity">

    <TextView
        android:id="@+id/go_fly_page_name"
        android:layout_width="183dp"
        android:layout_height="104dp"
        android:layout_marginTop="180dp"
        android:fontFamily="@font/montserrat_alternates"
        android:gravity="center"
        android:text="@string/go_fly_page_name"
        android:textColor="#000000"
        android:textSize="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/go_back_button"
        android:layout_width="32dp"
        android:layout_height="52dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="208dp"
        android:contentDescription="@string/go_back_button_desc"
        android:onClick="onBackClick"
        app:layout_constraintEnd_toStartOf="@+id/go_fly_page_name"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_go_back_button" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="128dp"
        android:layout_height="106dp"
        android:gravity="center"
        android:text="@string/development_text"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>